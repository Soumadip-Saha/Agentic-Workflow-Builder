{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/invoke.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/invoke",
      "methods": ["POST"],
      "dest": "/api/invoke"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}